export default function Home() {
  const [images, setImages] = useState([])
  const [analysis, setAnalysis] = useState(null)
  const [loading, setLoading] = useState(false)
  const [error, setError] = useState(null)
  const [uploadProgress, setUploadProgress] = useState(0)
  const [manualOverrides, setManualOverrides] = useState({
    gender: '',
    size: ''
  })

  // ‚úÖ ENHANCED: 24 Image Upload Handler
  const handleImageUpload = async (e) => {
    const files = Array.from(e.target.files)
    
    if (files.length === 0) return
    
    // ‚úÖ INCREASED LIMIT: 5 ‚Üí 24 images
    if (images.length + files.length > 24) {
      setError(`Maximum 24 images allowed. You can add ${24 - images.length} more images.`)
      return
    }

    setError(null)
    setUploadProgress(0)
    console.log(`üì∏ Processing ${files.length} new images...`)
    
    const imagePromises = files.map((file, index) => {
      return new Promise((resolve, reject) => {
        console.log(`üìÅ Processing ${file.name}: ${(file.size / 1024 / 1024).toFixed(2)}MB`)
        
        // Keep 15MB limit per file
        if (file.size > 15 * 1024 * 1024) {
          reject(new Error(`${file.name} is too large. Maximum size: 15MB`))
          return
        }

        const reader = new FileReader()
        reader.onload = (e) => {
          const img = new Image()
          img.onload = () => {
            const canvas = document.createElement('canvas')
            const ctx = canvas.getContext('2d')
            
            // ‚úÖ OPTIMIZED: Smaller compression for batch processing
            let { width, height } = img
            const maxSize = 500 // Further reduced for 24 images
            
            console.log(`üñºÔ∏è Original image ${index + 1}: ${width}x${height}`)
            
            if (width > maxSize || height > maxSize) {
              if (width > height) {
                height = (height * maxSize) / width
                width = maxSize
              } else {
                width = (width * maxSize) / height
                height = maxSize
              }
            }
            
            canvas.width = width
            canvas.height = height
            
            ctx.imageSmoothingEnabled = true
            ctx.imageSmoothingQuality = 'high'
            ctx.drawImage(img, 0, 0, width, height)
            
            // ‚úÖ MORE COMPRESSION: 50% quality for batch processing
            const compressedDataUrl = canvas.toDataURL('image/jpeg', 0.5)
            
            console.log(`‚úÖ Compressed image ${index + 1}: ${width}x${height}, Quality: 50%`)
            console.log(`üíæ Final size: ${(compressedDataUrl.length * 0.75 / 1024 / 1024).toFixed(2)}MB`)
            
            // Update progress
            setUploadProgress(prev => prev + (1 / files.length) * 100)
            
            resolve(compressedDataUrl)
          }
          
          img.onerror = () => {
            reject(new Error(`Failed to load image: ${file.name}`))
          }
          
          img.src = e.target.result
        }
        
        reader.onerror = () => {
          reject(new Error(`Failed to read file: ${file.name}`))
        }
        
        reader.readAsDataURL(file)
      })
    })

    try {
      const processedImages = await Promise.all(imagePromises)
      setImages(prev => [...prev, ...processedImages])
      console.log(`‚úÖ ${processedImages.length} new images processed and added`)
      
      // Calculate total compressed size
      const totalImages = [...images, ...processedImages]
      const totalSize = totalImages.reduce((total, img) => total + (img.length * 0.75 / 1024 / 1024), 0)
      console.log(`üìä Total collection size: ${totalSize.toFixed(2)}MB (${totalImages.length} images)`)
      
      setUploadProgress(0)
      
    } catch (error) {
      setError(error.message)
      console.error('‚ùå Image processing failed:', error)
      setUploadProgress(0)
    }
  }

  // ‚úÖ NEW: Remove individual image
  const removeImage = (index) => {
    setImages(prev => prev.filter((_, i) => i !== index))
    console.log(`üóëÔ∏è Removed image ${index + 1}`)
  }

  const analyzeImages = async () => {
    if (images.length === 0) {
      setError('Please upload at least one image')
      return
    }

    setLoading(true)
    setError(null)
    setAnalysis(null)

    try {
      console.log(`üöÄ Starting ENHANCED analysis for ${images.length} images...`)
      
      // ‚úÖ BATCH PROCESSING: Send all images at once
      const response = await fetch('/api/analyze-combined', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({
          images,
          manualOverrides
        })
      })

      if (!response.ok) {
        const errorData = await response.json()
        throw new Error(errorData.error || `Server error: ${response.status}`)
      }

      const result = await response.json()
      setAnalysis(result)
      
      console.log('‚úÖ ENHANCED Analysis complete!')
      console.log('üìä Enhanced features:')
      console.log('  ‚Ä¢ Images Analyzed:', images.length)
      console.log('  ‚Ä¢ Smart Pricing:', result.smart_pricing?.recommended_price ? `¬£${result.smart_pricing.recommended_price}` : 'Not available')
      console.log('  ‚Ä¢ Platform Titles:', Object.keys(result.platform_titles || {}).length, 'platforms')
      console.log('  ‚Ä¢ Market Intelligence:', result.market_positioning || 'Not available')
      console.log('  ‚Ä¢ Trend Detection:', result.detected_trends?.length || 0, 'trends')

